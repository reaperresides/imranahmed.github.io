{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/home",
      "dest": "/app.py"
    },
    {
      "src": "/portfolio",
      "dest": "/app.py"
    },
    {
      "src": "/download",
      "dest": "/app.py"
    }
  ]
}
